        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/btnInventario"
        android:layout_width="67dp"
        android:layout_height="64dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="48dp"
